    },
    {
      "Key": "stock",
      "Config": [
        {
          "Name": "Stock Service",
          "Version": "v1",
          "Url": "http://localhost:5502/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8080"
  }
}